{
  "$schema": "https://json.schemastore.org/vss-extension",
  "id": "advanced-library",
  "name": "Advanced Library",
  "manifestVersion": 1,
  "version": "1.0.3",
  "publisher": "dzhukovsky",
  "public": false,
  "author": "Dmitry Zhukovsky",
  "description": "Azure DevOps Extension",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/7.2"
  ],
  "icons": {
    "default": "icon.png"
  },
  "scopes": [
    "vso.variablegroups_manage",
    "vso.securefiles_read"
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Preview"
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/dzhukovsky/azdo-advanced-library"
  },
  "contributions": [
    {
      "id": "hub",
      "type": "ms.vss-web.hub",
      "targets": [
        "ms.vss-build-web.build-release-hub-group"
      ],
      "properties": {
        "name": "Advanced Library",
        "uri": "index.html",
        "iconName": "Library"
      }
    }
  ],
  "files": [
    {
      "path": "dist",
      "packagePath": "/",
      "addressable": true
    }
  ]
}